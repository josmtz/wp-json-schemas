{
    "namespace": "wp/v2",
    "methods": [
        "GET"
    ],
    "endpoints": [
        {
            "methods": [
                "GET"
            ],
            "args": {
                "stylesheet": {
                    "description": "The theme's stylesheet. This uniquely identifies the theme.",
                    "type": "string",
                    "required": false
                }
            }
        }
    ],
    "route": "/wp/v2/themes/(?P<stylesheet>[^.\\/]+(?:\\/[^.\\/]+)?)"
}